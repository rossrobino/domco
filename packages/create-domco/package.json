{
	"name": "create-domco",
	"description": "Create a new domco project",
	"version": "2.0.18",
	"type": "module",
	"types": "./dist/index.d.ts",
	"main": "./dist/index.js",
	"bin": "./dist/bin.js",
	"files": [
		"dist",
		"README.md",
		"LICENSE.md"
	],
	"scripts": {
		"test:clean": "rm -rf ./create-domco-test",
		"test": "npm run build && echo 'Create your project in `create-domco-test`\n\n' && node ./dist/bin.js",
		"dev": "tsc --watch",
		"build:clean": "rm -rf ./dist",
		"build:js": "tsc",
		"build": "npm run test:clean & npm run build:clean & wait && npm run build:js"
	},
	"author": {
		"name": "Ross Robino",
		"url": "https://robino.dev"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rossrobino/domco.git"
	},
	"license": "MIT",
	"dependencies": {
		"@clack/prompts": "^0.9.1",
		"domco": "^3.0.0",
		"prettier": "^3.4.2",
		"which-pm-runs": "^1.1.0"
	},
	"devDependencies": {
		"@types/which-pm-runs": "^1.0.2"
	}
}
